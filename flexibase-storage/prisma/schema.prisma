generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
}

model File {
    id           String   @id @default(uuid())
    originalName String   @map("original_name")
    mimeType     String   @map("mime_type")
    size         Int
    path         String
    bucket       String
    userId       String   @map("user_id")
    createdAt    DateTime @default(now()) @map("created_at")

    @@map("files")
}
